<!-- CONTENIDO INTERNAS -->
	<div class="container">
		<div class="contenido others">
			<div class="gridC">
				<h1>REGÍSTRATE</h1>
				<h4>Y gana increíbles premios al instante</h4>
					<h3>Tus datos</h3>
					<p>Paso 1 de 2</p>
						<form method="post" id="formStepOne" class="registerOreo">
						<label>Nombres y apellidos:</label>
						<input type="text" name="fullname" id="fullname" value="" />
						</br>
						<label>Documento de identidad:</label>
						<input type="text" name="identification" id="identification" value="" />
						</br>
						<div class="aaa">
						<label>Fecha de nacimiento:</label>
						<select class="day" name="day" id="day" value="">
						  <option value="">Día</option>
							<option value="1">10</option>				
						</select>
						<select class="month" name="month" id="month" value="">
						  <option value="">Mes</option>
							<option value="1">Septiembre</option>	
						</select>
						<select class="year" name="year" id="year" value="">
						  <option value="">Año</option>
							<option value="1">1987</option>	
						</select>
						</div>

						<label>Correo Electrónico:</label>
						<input type="text" name="email" id="email" value="" />
						</br>
						<label>Contraseña:</label>
						<input type="password" name="pass" id="pass" value="" />
						</br>
						<label>Repetir contraseña:</label>
						<input type="password" name="passto" id="passto" value="" />						
						<input class="NextForm" type="submit" value="SIGUIENTE">
						</form>
				</div>
		</div>
	</div>
